@article{hutcherson2011moral,
  ids = {hutcherson2011morala,hutcherson2011moralb},
  title = {The Moral Emotions: {{A}} Social\textendash Functionalist Account of Anger, Disgust, and Contempt.},
  shorttitle = {The Moral Emotions},
  author = {Hutcherson, Cendri A. and Gross, James J.},
  year = {2011},
  journal = {Journal of Personality and Social Psychology},
