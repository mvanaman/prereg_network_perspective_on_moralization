}

@article{horberg2009disgust,
  ids = {horberg2009disgusta},
  title = {Disgust and the Moralization of Purity.},
  author = {Horberg, E. J. and Oveis, Christopher and Keltner, Dacher and Cohen, Adam B.},
  year = {2009},
  journal = {Journal of Personality and Social Psychology},
  volume = {97},
  number = {6},
  pages = {963--976},
  issn = {1939-1315, 0022-3514},
  doi = {10.1037/a0017423},
  urldate = {2022-05-25},
  abstract = {Guided by appraisal-based models of the influence of emotion upon judgment, we propose that disgust moralizes\textemdash that is, amplifies the moral significance of\textemdash protecting the purity of the body and soul. Three studies documented that state and trait disgust, but not other negative emotions, moralize the purity moral domain but not the moral domains of justice or harm/care. In Study 1, integral feelings of disgust, but not integral anger, predicted stronger moral condemnation of behaviors violating purity. In Study 2, experimentally induced disgust, compared with induced sadness, increased condemnation of behaviors violating purity and increased approval of behaviors upholding purity. In Study 3, trait disgust, but not trait anger or trait fear, predicted stronger condemnation of purity violations and greater approval of behaviors upholding purity. We found that, confirming the domain specificity of the disgust\textendash purity association, disgust was unrelated to moral judgments about justice (Studies 1 and 2) or harm/care (Study 3). Finally, across studies, individuals of lower socioeconomic status (SES) were more likely than individuals of higher SES to moralize purity but not justice or harm/care.},
  langid = {english},
  file = {/Users/matthew/Zotero/storage/9JHT32G7/horberg2009disgust.pdf;/Users/matthew/Zotero/storage/WHX897XQ/horberg2009disgust.pdf}
}

