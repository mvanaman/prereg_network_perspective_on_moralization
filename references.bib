@article{royzman2014CAD,
  ids = {royzman2014cad,royzman2014cadb},
  title = {{{CAD}} or {{MAD}}? {{Anger}} (Not Disgust) as the Predominant Response to Pathogen-Free Violations of the Divinity Code.},
  shorttitle = {{{CAD}} or {{MAD}}?},
  author = {Royzman, Edward B. and Atanasov, Pavel and Landy, Justin F. and Parks, Amanda and Gepty, Andrew},
  year = {2014},
  month = oct,
  journal = {Emotion},
  volume = {14},
  number = {5},
